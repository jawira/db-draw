{
  "name": "jawira/db-draw",
  "description": "\ud83d\udcd0 Takes a DoctrineORM connection and generates a database diagram in .puml format",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "database",
    "db",
    "dbal",
    "diagram",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "Jawira Portugal"
    }
  ],
  "homepage": "https://jawira.github.io/db-draw/",
  "support": {
    "issues": "https://github.com/jawira/db-draw/issues"
  },
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.3 || ^4.0",
    "jawira/the-lost-functions": "^1.1",
    "jawira/doctrine-diagram-contracts": "^1.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "friendsofphp/php-cs-fixer": "^3.64",
    "jawira/plantuml": "^1.2024.6",
    "jawira/plantuml-client": "^1.0.3",
    "jawira/skeleton": "^2.24.1",
    "phpstan/phpstan": "^1.12.2",
    "phpunit/phpunit": "^9.6.20"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DbDraw\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\DbDrawTests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
