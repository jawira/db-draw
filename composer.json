{
  "name": "jawira/db-draw",
  "description": "\ud83d\udcd0 Library to generate database diagrams.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "database",
    "db",
    "dbal",
    "diagram",
    "doctrine",
    "php"
  ],
  "authors": [
    {
      "name": "Jawira Portugal"
    }
  ],
  "homepage": "https://jawira.github.io/db-draw/",
  "support": {
    "issues": "https://github.com/jawira/db-draw/issues"
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "doctrine/dbal": "^2.5 || ^3",
    "jawira/the-lost-functions": "^1.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ergebnis/composer-normalize": "^2.13",
    "friendsofphp/php-cs-fixer": "^3.0",
    "jawira/plantuml": "^1.59",
    "jawira/plantuml-client": "^1.0",
    "jawira/skeleton": "^2.0",
    "phpstan/phpstan": "^0.12.82",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DbDraw\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\DbDrawTests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "preferred-install": "dist"
  }
}
